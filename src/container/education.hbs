
  <div>
    {{#with main}}
  <main>
    <section>
      {{>title h1=true data="My education"}}
      <ul>
        {{#each educations}}
        <li>{{name}} 
            {{#unless isEnd}}  (ще в процесі)  {{/unless}}</li>
        {{/each}}
      </ul>
    </section>

    <section>
      {{>title h2=true data="My certificates"}}
      <ul>
        {{#each certificates}}
        <li>{{name}} ( {{id}} )
            </li>
        {{/each}}
      </ul>
    </section>
  </main>
{{/with}}
</div>
