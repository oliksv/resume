<div> <h1>{{name}}</h1>
          {{occupation}}<br>
        <br>
        Birthdate:{{birthdate}},BirthPlace:{{birthplace}},Nationality:{{nationality}}<br>
        DeathDate:{{deathdate}},DeathPlace:{{deathplace}}
        <h2>Known for:</h2>
            <ul>
              {{#each known_for}}
                <li>{{this}}</li>
              {{/each}}
            </ul>
      
     


              <p>
                {{#with education.[0]}}
                degree:{{degree}}
                field:{{field}}
                institution:{{institution}}
                year:{{year}}
                {{/with}}
              </p>
              <h3>Notable publications</h3>
            <ul>
              {{#each notable_publications}}
                <li>{{this.title}},{{this.year}},{{this.publisher}}</li>
              {{/each}}
            </ul>
              <h3>Partner</h3>
              <ul>
                {{#each partners}}
                  <li>
                  {{this.name}},{{this.relationship}},{{this.years}}</li>
                {{/each}}
              </ul>
                    <div>
                      <h3>Awards</h3>
                      <p>{{#with awards.[0] }}
                          {{title}},{{year}},{{description}}
                        {{/with}}
                      </p>
                    </div>
                {{>each title="Influences" data=influences}}
                {{>each title="Influenced" data=influenced}}
                {{>each title="Quotes" data=quotes}}
                  <div>
                    <h3>Major_discoveries</h3>
                    {{#with major_discoveries.[0]}}
                    {{title}},{{year}},{{description}}.
                    {{/with}}
                  </div>
                <div>
                  <h3>Contributions</h3>
                  {{#with contributions}}
                  {{title}},{{description}},{{year}},{{field}}.
                  {{/with}}
                </div>

</div>