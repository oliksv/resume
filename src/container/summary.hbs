<div>
  {{#with main}}
    <main>
    <section>
      {{>title big=true data=summary.title}}
      <div>
        <p>
          {{summary.text}}
        </p>
      </div>
    </section>

    <section>
      {{>title  big=false data=experience.title}}
       
       {{#with experience}}
      <div>
        <p>
          {{text}}
        </p>
      </div>
      {{/with}}
    </section>
  </main>
{{/with}}
</div>
