<div class='card'>
  <div class='card-body'>
    {{#with data}}
      <ul class='nav'>
        {{#each links}}
          <li class='nav-item'>
            <a
              class='nav-link'
              href={{this.href}}
            >{{this.text}}</a>
          </li>
        {{/each}}
      </ul>
    {{/with}}
  </div>
</div>