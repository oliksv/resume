<div class='mt-2'>
  <ul class='nav nav-tabs'>
    {{#each data}}
      <li class='nav-item'>
        {{#if this.isActive}}
          <a
            class='nav-link active'
            aria-current='page'
            href='#'
          >{{this.text}}</a>
        {{else}}
          <a
            class='nav-link'
            aria-current='page'
            href='#'
          >{{this.text}}</a>
        {{/if}}
      </li>
    {{/each}}
  </ul>
</div>