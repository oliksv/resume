<section class='card' style='width: 18rem;'>
  <img
    src={{data.picture.src}}
    class='card-img-top'
    alt={{data.picture.alt}}
  />
  <div class='card-body'>
    <h5 class='card-title'>{{data.title}}</h5>
    <p class='card-text'>{{data.description}}</p>
    <a
      href={{data.button.href}}
      class='btn btn-primary'
    >{{data.button.text}}</a>
  </div>
</section>